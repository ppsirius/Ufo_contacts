<h1>All contacts with aliens</h1>

<table>
  <thead>
    <tr>
      <th>City</th>
      <th>Contact</th>
      <th>Contact num</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contact_all.each do |contact| %>
      <tr>
        
      	<td><%= contact.city.name %></td>
        <td><%= contact.contact_num %></td>

      </tr>
   <% end %>
  </tbody>
</table>

<br>


