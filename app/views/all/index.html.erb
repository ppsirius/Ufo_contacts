<h1>All contacts with aliens</h1>

<table>
  <thead>
    <tr>
      <th>City</th>
      <th>Contact</th>
      <th>Contact num</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cities.each do |city| %>
      <tr>
        
      	<td><%= city.name %></td>
        <td><%= city.contacts.count %></td>
        <td><%= city.contacts.sum(:contact_num) %></td>

      </tr>
   <% end %>
  </tbody>
</table>

<br>


